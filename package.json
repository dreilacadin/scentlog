{
  "name": "scentlog",
  "version": "0.1.9",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 4000",
    "prisma:studio": "prisma studio",
    "prisma:studio:test": "dotenv -e .env.test prisma studio",
    "seed": "NODE_ENV=development prisma db seed",
    "seed:test": "NODE_ENV=development dotenv -e .env.test prisma db seed",
    "db:push": "prisma db push",
    "db:push:test": "dotenv -e .env.test prisma db push",
    "db:reset": "NODE_ENV=development prisma migrate reset",
    "db:reset:test": "NODE_ENV=development dotenv -e .env.test prisma migrate reset",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "test": "dotenv -e .env.test yarn jest -- -u",
    "prepare": "husky install",
    "release": "release-it --only-version"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^6.0.2",
    "@mantine/dropzone": "^6.0.2",
    "@mantine/form": "^6.0.2",
    "@mantine/hooks": "^6.0.2",
    "@mantine/modals": "^6.0.2",
    "@mantine/next": "^6.0.2",
    "@mantine/notifications": "^6.0.2",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.11.0",
    "@tabler/icons-react": "^2.10.0",
    "@tanstack/react-query": "^4.27.0",
    "@trpc/client": "^10.16.0",
    "@trpc/next": "^10.16.0",
    "@trpc/react-query": "^10.16.0",
    "@trpc/server": "^10.16.0",
    "add": "^2.0.6",
    "cookies-next": "^2.1.1",
    "next": "13.2.4",
    "next-auth": "^4.20.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.12.2",
    "trpc-panel": "^1.2.8",
    "yarn": "^1.22.19",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@faker-js/faker": "^7.6.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@testing-library/react": "^14.0.0",
    "@types/eslint": "^8.21.2",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "cuid": "^3.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-next": "13.2.4",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-mock-extended": "^3.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "prisma": "^4.11.0",
    "release-it": "^15.9.0",
    "tailwindcss": "^3.2.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.5",
    "typescript": "^4.8.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.5.6"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release: v${version}",
      "changelog": "npx auto-changelog --stdout --commit-limit false -u --template https://raw.githubusercontent.com/release-it/release-it/main/templates/keepachangelog.hbs"
    },
    "hooks": {
      "after:bump": "npx auto-changelog -p"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  }
}
